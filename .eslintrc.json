{
  "extends": ["next/core-web-vitals", "next/typescript"],
  "rules": {
    "react-refresh/only-export-components": "off",
    "no-empty-pattern": "off",
    "@typescript-eslint/no-unused-vars": [
      "warn",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": true
      }
    ],
    "no-console": ["warn", { "allow": ["warn", "error"] }]
  }
}
